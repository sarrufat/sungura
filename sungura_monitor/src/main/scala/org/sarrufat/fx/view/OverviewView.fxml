<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.sarrufat.fx.controller.OverviewController">
   <children>
      <GridPane layoutX="10.0" layoutY="10.0" prefHeight="306.0" prefWidth="593.0">
         <children>
            <LineChart fx:id="msgRatesChart" title="Message rates" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
               <xAxis>
                  <CategoryAxis label="Hora" side="BOTTOM" />
               </xAxis>
               <yAxis>
                  <NumberAxis label="M/S" side="LEFT" />
               </yAxis>
            </LineChart>
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
   </children>
</AnchorPane>
